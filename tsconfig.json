{
  "$schema": "https://json.schemastore.org/tsconfig",
  "_version": "22.0.0",
  "include": ["src/**/*.ts"],
  "compilerOptions": {
    "lib": ["es2023"],
    "module": "commonjs",
    "target": "es2022",
    "types": ["node"],


    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node16"
  }
}